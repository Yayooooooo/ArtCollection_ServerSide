{
  "name": "poetryweb",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "esw  ./test ./routes ./models/ app.js bin/www ",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --watch",
    "test": "cross-env NODE_ENV=test mocha",
    "test:report": "nyc mocha",
    "compile": "babel ./ --out-dir dist/ --ignore ./node_modules,./test --copy-files ",
    "start:prod": "cross-env NODE_ENV=dev node dist/bin/www",
    "start:watch": "cross-env NODE_ENV=dev nodemon --exec babel-node bin/www",
    "prebuild:dev": "npm-run-all test",
    "build:dev": "npm run compile",
    "postbuild:dev": "npm run start:prod",
    "build:release": "npm-run-all  test compile ",
    "postbuild:release": "rimraf dist/.git dist/test dist/nodemon.json"
  },
  "nyc": {
    "reporter": [
      "text-summary",
      "html",
      "cobertura"
    ],
    "sourceMap": true,
    "instrument": true,
    "extension": [
      ".ts"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai": "^4.0.2",
    "chai-http": "^4.3.0",
    "chai-things": "^0.2.0",
    "connect-mongo": "^3.1.0",
    "cookie-parser": "~1.4.4",
    "cors": "latest",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "json-server": "^0.15.1",
    "lodash": "^4.17.21",
    "mocha": "^6.2.0",
    "mongodb": "^3.3.3",
    "mongoose": "^5.7.7",
    "morgan": "~1.9.1",
    "nom": "0.0.3",
    "serve-favicon": "^2.5.0",
    "superagent": "^5.1.0",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.2",
    "eslint-watch": "^6.0.1",
    "express-session": "^1.17.0",
    "mock-session": "0.0.5",
    "mongodb-memory-server": "^6.0.1",
    "multer": "^1.4.2",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "rimraf": "^2.7.1"
  }
}
